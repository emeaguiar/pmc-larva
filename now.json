{
	"version": 2,
    "name": "larva",
    "builds": [
        {
            "src": "**/*.js",
            "use": "@now/node"
        },
		{
			"src": "packages/larva-js/build/**",
			"use": "@now/static"
		},
		{
			"src": "packages/larva/static/**",
			"use": "@now/static"
		}

    ],
	"routes": [{
		"src": "/(.*)",
		"dest": "/packages/larva/lib/server.js"
	}]
}
